    <%- include("header") %> 
    <div class="box" id="heading">
      <h1><%= listTitle %></h1>
    </div>
    <div class="box">
      <div class="item">
        <input type="checkbox" />
        <% items.map((item)=>{ %>
        <p><%= item %></p>
        <% }) %>
      </div>
      <form method="post" action="/">
        <input
          type="text"
          name="newItem"
          placeholder="New Item"
          autocomplete="off"
        />
        <button type="submit" name="list" value=<%=listTitle%>>+</button>
      </form>
    </div>
  <%-include("footer")%>
